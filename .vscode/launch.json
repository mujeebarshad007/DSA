{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C++ Launch Current File",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Use decimal display for numbers",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Show numbers in decimal",
                    "text": "set output-radix 10",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
